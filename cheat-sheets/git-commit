git checkout -b git-commit
echo "This is a sample Git repository used in my *You don't know Git* talk." >> README.md
git add README.md
git commit -m 'WIP'
git status
echo >> README.md
echo 'Feel free to clone the repository and run through the *cheat-sheets*.' >> README.md
git add README.md
git commit --amend
git status
git log --oneline -5
git commit --amend --no-edit
